# Copy this file to .env and fill in your API keys:
#   cp .env.example .env

# Hume AI — for live voice-to-voice (Tab 3)
HUME_API_KEY=your_hume_api_key_here
HUME_SECRET_KEY=your_hume_secret_key_here
HUME_CONFIG_ID=your_hume_config_id_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI ChatGPT
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# LLM Provider — "claude" or "openai" (default for CLM server)
LLM_PROVIDER=claude

# Server settings (usually no need to change)
CLM_HOST=0.0.0.0
CLM_PORT=8000
STREAMLIT_PORT=8501
